
let cadastroidade = 16

if(cadastroidade < 18){
    console.log("Idade não permitida, evento para maiores de 18 anos")
}else{
    console.log("Idade permitida")
}

let datadoevento = 30/07/2022

if(datadoevento < 30/07/2022){
    console.log("Data permitida")
}else{
    console.log("Data não permitida")
}    

var listaparticipantes = ("João","Pedro","Mariana","Alice","Roberta")

if(listadeparticipantes.length < 100 ){
    console.log("cadastro permitido")
}else{
    console.log("Cadastro Negado. Limite de participantes atingido")
}